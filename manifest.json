{
  "manifest_version": 3,
  "name": "Weda Helper",
  "version": "2.11.3",
  "version_name": "2.11.3",
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "optional_permissions": [
    "tabs"
  ],
  "host_permissions": [
    "http://localhost/"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "description": "Simplifie et fluidifie l'usage de Weda",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://secure.weda.fr/*"
      ],
      "js": [
        "metrics.js",
        "lib/hotkeys.js",
        "lib/pdf-lib.js",
        "utils.js",
        "content.js",
        "tooltip.js",
        "popupFunctions.js",
        "patientLink.js",
        "accueil.js",
        "consultation.js",
        "fse.js",
        "keyCommands.js",
        "searchUpload.js",
        "companionLink.js",
        "prescription.js",
        "update.js",
        "aati.js",
        "upload.js",
        "agenda.js",
        "antecedent.js",
        "print.js",
        "courrier.js",
        "imti.js",
        "wedaEchanges.js",
        "pdfParser.js",
        "optionalPermissions.js",
        "headlessSetup.js",
        "instantVaccine.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "FW_scripts/*",
        "lib/*",
        "CHANGELOG.md",
        "README.md",
        "Images/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
